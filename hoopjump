import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
    
    public static void lastHoop(int n)
    {
        int out=0;
        if(n==1)
         {
             out=1;
             
         }
         else if(n%2==0)
         { out =n/2 ;}
         else{
             out=(n/2)+1;
         }
         
         System.out.println(out);
        
    }
    
	public static void main (String[] args) throws java.lang.Exception
	{
	   try{ Scanner in = new Scanner(System.in);
	    int test = in.nextInt();
	    
	    while(test-->0)
	    {
	        int n = in.nextInt();
	        lastHoop(n); 
	       
	    }}catch(Exception e)
	    {
	        
	    }
		
	}
}
